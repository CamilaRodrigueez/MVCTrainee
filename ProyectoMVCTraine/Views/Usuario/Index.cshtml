@model IEnumerable<Infracestructure.Entity.Entity.UsuarioEntity>



@{

}
<h1>Lista de Usuario</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">User Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Name</td>
            <td>@item.LastName</td>
            <td scope="row">@item.UserName</td>
        </tr>

        }


    </tbody>
</table>